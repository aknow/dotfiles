[ui]
username = Szu-Yu Chen [:aknow] <szchen@mozilla.com>
editor=vim

[defaults]
diff = -pU8
qdiff = -pU8
qnew = -UDe
qseries = -sv

[extensions]
color =
hgext.graphlog =
hgext.mq =
hgext.record =
hgext.rebase =
churn =
trychooser = ~/local/trychooser/trychooser
hgext.purge=

[diff]
git = 1
showfunc = 1
unified = 8

[alias]
addtry = qnew -m  "try: -b do -p emulator,panda,unagi -u xpcshell,marionette,mochitests,marionette-webapi -t none" try
addmtry = qnew -m "try: -b o -p emulator,panda,unagi -u marionette,marionette-webapi -t none" try
addxtry = qnew -m "try: -b o -p emulator,panda,unagi -u xpcshell -t none" try
try     = push -f ssh://hg.mozilla.org/try
